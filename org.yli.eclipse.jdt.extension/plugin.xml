<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            description="JDT Extension"
            id="org.yli.eclipse.jdt.extension.category"
            name="%JDT_EXTENSION">
      </category>
      <command
            categoryId="org.yli.eclipse.jdt.extension.category"
            defaultHandler="org.yli.eclipse.jdt.extension.handlers.GenerateGuavaToStringHandler"
            id="org.yli.eclipse.jdt.extension.generate_guava_to_string"
            name="%GENERATE_TO_STRING">
      </command>
      <command
            categoryId="org.yli.eclipse.jdt.extension.category"
            defaultHandler="org.yli.eclipse.jdt.extension.handlers.GenerateGuavaHashcodeAndEqualsHandler"
            id="org.yli.eclipse.jdt.extension.generate_guava_hashcode_equals"
            name="%GENERATE_HASHCODE_EQUALS">
      </command>
      <command
            description="%SEARCH_WITH_GOOGLE"
            id="org.yli.eclipse.jdt.extension.terminal_search_with_google"
            name="%SEARCH_WITH_GOOGLE">
      </command>
      <command
            description="%SEARCH_WITH_GOOGLE"
            id="org.yli.eclipse.jdt.extension.console_search_with_google"
            name="%SEARCH_WITH_GOOGLE">
      </command>
      <command
            description="%SEARCH_WITH_STACKOVERFLOW"
            id="org.yli.eclipse.jdt.extension.terminal_search_with_stackoverflow"
            name="%SEARCH_WITH_STACKOVERFLOW">
      </command>
      <command
            description="%SEARCH_WITH_STACKOVERFLOW"
            id="org.yli.eclipse.jdt.extension.console_search_with_stackoverflow"
            name="%SEARCH_WITH_STACKOVERFLOW">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.GenerateGuavaToStringHandler"
            commandId="org.yli.eclipse.jdt.extension.generate_guava_to_string">
      </handler>
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.GenerateGuavaHashcodeAndEqualsHandler"
            commandId="org.yli.eclipse.jdt.extension.generate_guava_hashcode_equals">
      </handler>
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.SearchWithGoogleHandlerInTerminal"
            commandId="org.yli.eclipse.jdt.extension.terminal_search_with_google">
      </handler>
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.SearchWithStackOverflowHandlerInConsole"
            commandId="org.yli.eclipse.jdt.extension.console_search_with_stackoverflow">
      </handler>
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.SearchWithStackOverflowHandlerInTerminal"
            commandId="org.yli.eclipse.jdt.extension.terminal_search_with_stackoverflow">
      </handler>
      <handler
            class="org.yli.eclipse.jdt.extension.handlers.SearchWithGoogleHandlerInConsole"
            commandId="org.yli.eclipse.jdt.extension.console_search_with_google">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="%JDT_EXTENSION">
            <command
                  commandId="org.yli.eclipse.jdt.extension.generate_guava_to_string"
                  label="%GENERATE_TO_STRING"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <test
                                 property="org.eclipse.core.resources.name"
                                 value="*.java">
                           </test>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <command
                  commandId="org.yli.eclipse.jdt.extension.generate_guava_hashcode_equals"
                  label="%GENERATE_HASHCODE_EQUALS"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <test
                                 property="org.eclipse.core.resources.name"
                                 value="*.java">
                           </test>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.tm.terminal.view.ui.TerminalsView?after=additions">
         <command
               commandId="org.yli.eclipse.jdt.extension.terminal_search_with_google"
               icon="icons/google-icon.png"
               label="%SEARCH_WITH_GOOGLE"
               mnemonic="g"
               style="push">
         </command>
         <command
               commandId="org.yli.eclipse.jdt.extension.terminal_search_with_stackoverflow"
               icon="icons/stackoverflow-icon.png"
               label="%SEARCH_WITH_STACKOVERFLOW"
               mnemonic="s"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.debug.ui.ProcessConsoleType.#ContextMenu">
         <command
               commandId="org.yli.eclipse.jdt.extension.console_search_with_google"
               icon="icons/google-icon.png"
               label="%SEARCH_WITH_GOOGLE"
               mnemonic="g"
               style="push">
         </command>
         <command
               commandId="org.yli.eclipse.jdt.extension.console_search_with_stackoverflow"
               icon="icons/stackoverflow-icon.png"
               label="%SEARCH_WITH_STACKOVERFLOW"
               mnemonic="g"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.MessageConsole.#ContextMenu">
         <command
               commandId="org.yli.eclipse.jdt.extension.console_search_with_google"
               icon="icons/google-icon.png"
               label="%SEARCH_WITH_GOOGLE"
               mnemonic="g"
               style="push">
         </command>
         <command
               commandId="org.yli.eclipse.jdt.extension.console_search_with_stackoverflow"
               icon="icons/stackoverflow-icon.png"
               label="%SEARCH_WITH_STACKOVERFLOW"
               mnemonic="g"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.yli.eclipse.jdt.extension.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.yli.eclipse.jdt.extension.generate_guava_to_string"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.yli.eclipse.jdt.extension.scheme"
            sequence="M1+T">
      </key>
      <scheme
            id="org.yli.eclipse.jdt.extension.scheme"
            name="%JDT_EXTENSION">
      </scheme>
      <key
            commandId="org.yli.eclipse.jdt.extension.generate_guava_hashcode_equals"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.yli.eclipse.jdt.extension.scheme"
            sequence="M1+H">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="org.yli.eclipse.jdt.extension.googlesearch"
            nameFilter="*.*"
            objectClass="org.eclipse.ui.IEditorInput">
         <menu
               icon="icons/google-icon.png"
               id="org.yli.eclipse.jdt.extension.menu_google_search"
               label="%SEARCH_WITH_GOOGLE"
               path="additions">
         </menu>
         <action
               class="org.yli.eclipse.jdt.extension.actions.SearchWithGoogleAction"
               icon="icons/google-icon.png"
               id="org.yli.eclipse.jdt.extension.action_google_search"
               label="%SEARCH_WITH_GOOGLE"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.yli.eclipse.jdt.extension.stackoverflowsearch"
            nameFilter="*.*"
            objectClass="org.eclipse.ui.IEditorInput">
         <menu
               icon="icons/stackoverflow-icon.png"
               id="org.yli.eclipse.jdt.extension.menu_stackoverflow_search"
               label="%SEARCH_WITH_STACKOVERFLOW"
               path="additions">
         </menu>
         <action
               class="org.yli.eclipse.jdt.extension.actions.SearchWithStackOverflowAction"
               icon="icons/stackoverflow-icon.png"
               id="org.yli.eclipse.jdt.extension.action_stackoverflow_search"
               label="%SEARCH_WITH_STACKOVERFLOW"
               style="push">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="org.yli.eclipse.jdt.extension.notemaker"
            nameFilter="*.*"
            objectClass="org.eclipse.ui.IEditorInput">
         <menu
               id="org.yli.eclipse.jdt.extension.menu_make_note"
               label="%MAKE_A_NOTE"
               path="additions">
         </menu>
         <action
               class="org.yli.eclipse.jdt.extension.actions.MakeNoteAction"
               id="org.yli.eclipse.jdt.extension.menu_make_note"
               label="%MAKE_A_NOTE"
               style="push">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.core.filebuffers.annotationModelCreation">
      <factory
            class="org.yli.eclipse.jdt.extension.note.NoteAnnotationModelFactory"
            extensions="java">
      </factory>
   </extension>
   <extension
         id="org.yli.eclipse.jdt.note.marker"
         name="Note Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationImageProvider="org.yli.eclipse.jdt.extension.note.NoteAnnotationImageProvider"
            annotationType="org.yli.eclipse.jdt.extension.noteSpecification"
            colorPreferenceKey="note_color"
            colorPreferenceValue="255,255,0"
            contributesToHeader="false"
            highlightPreferenceKey="note_highlight"
            highlightPreferenceValue="true"
            includeOnPreferencePage="true"
            isGoToNextNavigationTarget="false"
            isGoToNextNavigationTargetKey="note_isGoToNextNavigationTargetKey"
            isGoToPreviousNavigationTarget="false"
            isGoToPreviousNavigationTargetKey="note_isGoToPreviousNavigationTargetKey"
            label="%NOTE_ANNOTATION_LABEL"
            overviewRulerPreferenceKey="note_overviewRuler"
            overviewRulerPreferenceValue="false"
            presentationLayer="0"
            showInNextPrevDropdownToolbarAction="false"
            showInNextPrevDropdownToolbarActionKey="note_showInNextPrevDropdownToolbarAction"
            textPreferenceKey="note_text"
            textPreferenceValue="true"
            textStylePreferenceKey="note_stylePreferences"
            textStylePreferenceValue="BOX"
            verticalRulerPreferenceKey="note_verticalRuler"
            verticalRulerPreferenceValue="true">
      </specification>
   </extension>

</plugin>
